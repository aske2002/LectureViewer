  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "name": "Frontend",
      "request": "launch",
      "command": "yarn dev",
      "cwd": "${workspaceFolder}/Client"
    },
    {
      "requireExactSource": false,
      "name": "Backend",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build",
      "program": "${workspaceFolder}/backend/src/Web/bin/Debug/net9.0/backend.Web.dll",
      "cwd": "${workspaceFolder}/backend/src/Web",
      "args": [],
      "stopAtEntry": false
    }
  ],
  "compounds": [
    {
        "name": "Compound",
        "configurations": ["Backend", "Frontend"]
    }
  ]
}
